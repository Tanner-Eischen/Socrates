{
  "name": "socrateach",
  "version": "4.0.0",
  "description": "Production-ready multi-modal Socratic learning platform with real-time collaboration and advanced analytics",
  "main": "dist/index.js",
  "bin": {
    "tutor": "bin/socrateach",
    "socrateach": "bin/socrateach"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli-tester.js",
    "dev": "ts-node src/api/index.ts",
    "test": "ts-node src/cli-tester.ts",
    "watch": "nodemon --exec ts-node src/cli-tester.ts",
    "api": "node dist/api/index.js",
    "api:dev": "ts-node src/api/index.ts",
    "api:watch": "nodemon --exec ts-node src/api/index.ts",
    "server": "npm run api",
    "server:dev": "npm run api:dev",
    "server:watch": "npm run api:watch",
    "demo": "npm run build && npm start",
    "check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test:unit": "jest",
    "test:api": "jest --config jest.api.config.js",
    "migrate": "ts-node src/database/migrate.ts",
    "postinstall": "echo Skipping postinstall build in development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tanner-Eischen/SocraTeach.git"
  },
  "keywords": [
    "education",
    "socratic-method",
    "ai-tutoring",
    "collaboration",
    "analytics"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tanner-Eischen/SocraTeach/issues"
  },
  "homepage": "https://github.com/Tanner-Eischen/SocraTeach#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "@types/node": "^24.10.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^6.7.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^8.1.0",
    "redis": "^4.6.12",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.9.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^3.0.13",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  }
}
